{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions":[
    {
      "name": "api",
      "image": "jiangwensi/mrbs-backend",
      "hostname": "api",
      "essential": true,
      "memory": 128,
      "volumes": [
        {
          "name": "mrbs_backend_log",
          "host": {
            "sourcePath": "/mrbs/log"
          }
        },{
          "name": "mrbs_backend_conf",
          "host": {
            "sourcePath": "/mrbs/config"
          }
        }
      ]
    },
    {
      "name": "ui",
      "image": "jiangwensi/mrbs-ui",
      "hostname": "ui",
      "essential": false,
      "memory": 128,
      "Ports": [
      {
        "ContainerPort": 3000,
        "HostPort": 3000
      }
    ]
    }
  ]
}