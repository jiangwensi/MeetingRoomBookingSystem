{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions":[
    {
      "name": "api",
      "image": "jiangwensi/mrbs-backend",
      "hostname": "api",
      "essential": true,
      "memory": 128
    },
    {
      "name": "ui",
      "image": "jiangwensi/mrbs-ui",
      "hostname": "ui",
      "essential": false,
      "memory": 128,
      "Ports": [
      {
        "ContainerPort": 3000,
        "HostPort": 3000
      }
    ]
    }
  ]
}