{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions":[
    {
      "name": "api",
      "image": "jiangwensi/mrbs-backend",
      "hostname": "api",
      "essential": false,
      "memory": 128
    },
    {
      "name": "ui",
      "image": "jiangwensi/mrbs-ui",
      "hostname": "ui",
      "essential": false,
      "memory": 128,
      "Ports": [
        {
          "ContainerPort": 3000,
          "HostPort": 3000
        }
      ]
    },
    {
      "name":"nginx",
      "image":"jiangwensi/mrbs-nginx",
      "hostname":"nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "ui"
      ],
      "memory": 128
    }
  ]
}